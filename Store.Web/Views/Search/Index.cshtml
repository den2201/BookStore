@model IEnumerable<store.Book>

@{
    ViewData["Title"] = "Serch results";
}

<h2>Results:</h2>
@if (Model.Count() == 0)
{
    <p>Book not found</p>
}
else
{ 
    <ul>
        @foreach(var book in Model)
        {
            <li>
                <a href="@Url.Action("Index","Book",new {id = book.Id })">@book.Title</a>
            </li>
        }

    </ul>
}

